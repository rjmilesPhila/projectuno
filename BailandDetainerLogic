SELECT 
(SELECT PPN 
FROM OFFENDERS O
WHERE O.OFFENDER = D.OFFENDER
LIMIT 1) AS  PPN,
docketno, bail, S.DETAINERNO

FROM Dockets D , Detainers S
WHERE D.offender = 10574045 
AND
BAIL IS NOT NULL
AND
D.OFFENDER = S.OFFENDER

--AND 
NOT EXISTS

(SELECT *

FROM dockets
 

WHERE offender = 10574045 
AND BAIL IS NULL 
LIMIT 1)
